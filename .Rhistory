Plot1 <-
PakPC2023Pak %>%
fmutate(Region = as_factor(Region), Area = as_factor(Area)) %>%
fsubset(Region %!in% c("Pakistan", "ICT") & Area == "All") %>%
ggplot(
data = .
, mapping = aes(x = fct_rev(Region), y = Pop2023, fill = fct_rev(Area))
) +
geom_bar(position = "dodge", stat = "identity") +
geom_text(
mapping  =  aes(label = unit_format(unit = "M", scale = 1e-6)(Pop2023))
, position = position_dodge(width = 0.9)
, hjust    = -0.1
) +
scale_y_continuous(
expand = c(0, 0)
, labels = label_number(scale_cut = cut_si(""))
, breaks = pretty_breaks(8)
, limits = c(0, 82000000)
) +
coord_flip() +
labs(
x   = "Provinces"
, y    = "Population 2023 (in Millions)"
, fill = "Area"
) +
guides(fill = guide_legend(reverse = TRUE)) +
theme_classic()
Plot1
